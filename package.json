{
  "name": "boilerplate-api-express-node",
  "version": "1.0.0",
  "description": "A",
  "main": "index.js",
  "scripts": {
    "prestart": "NODE_ENV=development ./node_modules/.bin/sequelize db:migrate",
    "start": "babel-node ./src/index.js",
    "pm2": "pm2 delete ecosystem.json; pm2 startOrReload ecosystem.json --interpreter babel-node",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "prebuild": "rm -rf ./dist",
    "build": "./node_modules/.bin/babel ./src --out-dir dist/",
    "prestartbuilded": "./node_modules/.bin/babel ./src --out-dir dist/",
    "startbuilded": "node ./dist/index.js",
    "debug": "babel-node ./src/index.js --nolazy --inspect-brk=9229",
    "pretest": "rm -f ./database/db.test.sqlite; NODE_ENV=test ./node_modules/.bin/sequelize db:migrate; NODE_ENV=test node_modules/.bin/sequelize db:seed:all",
    "test": "NODE_ENV=test mocha test --exit",
    "cover": "cross-env NODE_ENV=test nyc mocha test --exit",
    "coverreport": "nyc report --reporter=lcov",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "nyc": {
    "exclude": [
      "test",
      "dist",
      "database",
      "log",
      "public",
      "./src/domain",
      "./src/config"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "consign": "^0.1.6",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.3",
    "express-winston": "^2.4.0",
    "moment": "^2.21.0",
    "passport": "^0.4.0",
    "passport-hash": "^0.6.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.22.11",
    "sqlite3": "^4.0.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-istanbul": "0.12.2",
    "babel-plugin-istanbul": "4.1.6",
    "babel-register": "6.26.0",
    "coveralls": "3.0.0",
    "cross-env": "5.1.4",
    "eslint": "4.19.1",
    "istanbul": "0.4.5",
    "mocha": "5.0.5",
    "nyc": "11.6.0",
    "sequelize-cli": "4.0.0",
    "supertest": "3.0.0"
  }
}
